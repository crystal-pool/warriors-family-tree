{
    "rules": {
        "no-duplicate-imports": true,
        "no-duplicate-variable": true,
        "no-import-side-effect": [
            true,
            {
                "ignore-module": "\\.(css|sass|scss)$"
            }
        ],
        "ordered-imports": true,
        "eofline": true,
        "await-promise": [
            true,
            "Promise",
            "PromiseLike"
        ],
        "quotemark": [
            true,
            "double"
        ],
        "no-var-keyword": true,
        "semicolon": true,
        "member-access": true,
        "no-floating-promises": true,
        "object-literal-key-quotes": [
            true,
            "as-needed"
        ]
    }
}